{
  "name": "9dt",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Autumn Apsey <autumnapsey@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env production",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --mode development\"",
    "prepush": "?",
    "precommit": "lint-staged",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "recompose": "^0.26.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "nodemon": "^1.17.2",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.11.1",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended",
      "prettier",
      "prettier/flowtype",
      "prettier/react"
    ],
    "rules": {
      "react/no-string-refs": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "parser": "flow",
          "trailingComma": "all"
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-wrap-multilines": "off",
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    },
    "plugins": [
      "prettier",
      "flowtype"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "fetch": true
    }
  }
}
